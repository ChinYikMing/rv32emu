---
 package/Config.in                  |   4 +
 package/smolnes/Config.in          |   6 +
 package/smolnes/smolnes.mk         |  13 +
 package/smolnes/src/.smolnes.c.swp | Bin 0 -> 45056 bytes
 package/smolnes/src/Makefile       |   2 +
 package/smolnes/src/falling-nes.h  | 106 ++++
 package/smolnes/src/smolnes.c      | 894 +++++++++++++++++++++++++++++
 7 files changed, 1025 insertions(+)
 create mode 100644 package/smolnes/Config.in
 create mode 100644 package/smolnes/smolnes.mk
 create mode 100644 package/smolnes/src/.smolnes.c.swp
 create mode 100644 package/smolnes/src/Makefile
 create mode 100644 package/smolnes/src/falling-nes.h
 create mode 100644 package/smolnes/src/smolnes.c

diff --git a/package/Config.in b/package/Config.in
index 978f6d1a..95e75868 100644
--- a/package/Config.in
+++ b/package/Config.in
@@ -2906,4 +2906,8 @@ menu "Mem_test Packages"
     source "package/mem_test/Config.in"
 endmenu
 
+menu "Smolnes Packages"
+    source "package/smolnes/Config.in"
+endmenu
+
 endmenu
diff --git a/package/smolnes/Config.in b/package/smolnes/Config.in
new file mode 100644
index 00000000..f9b6c2f6
--- /dev/null
+++ b/package/smolnes/Config.in
@@ -0,0 +1,6 @@
+config BR2_PACKAGE_SMOLNES
+    bool "smolnes"
+    help
+        smolnes package.
+
+        https://smolnes.com
diff --git a/package/smolnes/smolnes.mk b/package/smolnes/smolnes.mk
new file mode 100644
index 00000000..39350f83
--- /dev/null
+++ b/package/smolnes/smolnes.mk
@@ -0,0 +1,13 @@
+SMOLNES_VERSION =
+SMOLNES_SITE = package/smolnes/src
+SMOLNES_SITE_METHOD = local
+
+define SMOLNES_BUILD_CMDS
+        $(MAKE) CC="$(TARGET_CC)" LD="$(TARGET_LD)" -C $(@D)/
+endef
+
+define SMOLNES_INSTALL_TARGET_CMDS
+        $(INSTALL) -D -m 0755 $(@D)/smolnes $(TARGET_DIR)/usr/bin
+endef
+
+$(eval $(generic-package))
